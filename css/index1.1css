}
.pricename{
    display: flex;
    margin-top: 1rem;
    margin-left: 2.5rem;
}
.pricename p{
    margin-left: 1rem;
    margin-top: 0.1rem;
    font-size: 22px;
    font-weight: 800;
    text-align: center;
}
.price h2{
    font-size: 22px;
    font-family: 'Quicksand', serif;
