    box-shadow: 2px 2px 30px rgba(0,0,0,0.2);
}
.produk{
    height: auto;
    min-height: 0px;
    padding-right: 0px;
    display: block;
    margin-top: 5rem;
    width: 100%;   
}
.title-produk{
    float: left;
    font-family: sans-serif;
    font-weight: 600;
    margin-left: 2.5rem;
}
.box-produk{
    position: relative;
    margin-top: 3rem;
    margin-left: 2.5rem;
    margin-right: 2.5rem;
    height: auto;
    padding: 30px 20px 0px;
    border-radius: 20px;
    padding-bottom: 1.5rem;
    background: #dadada;
