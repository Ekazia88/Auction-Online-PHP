    padding-right: 2rem;
    padding-bottom: 2rem;
    box-shadow: 5px 5px -5px rgb(0 0 0 / 10%);
}
.box-desc h2{
    margin-bottom: .5rem;
}
.box2{
    position: relative;
    margin-top: 30px;
    display: flex;
}
.box-bid-section{
    display: block;
